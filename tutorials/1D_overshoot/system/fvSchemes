FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
    default         Gauss linear;
    
    div(phi,U)              Gauss linearUpwind grad(U);
    div(phiHbyA,Sw)         Gauss vanLeer;
}

laplacianSchemes
{
    default         Gauss cubic limited 1.0;
}

interpolationSchemes
{
    default         linear; 
}

snGradSchemes
{
    default         limited 1.0;
}

fluxRequired
{
  Sw;
}

