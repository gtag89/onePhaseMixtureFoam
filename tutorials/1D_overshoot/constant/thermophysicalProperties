FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          hConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    specie
    {
        nMoles      1;
        molWeight   18;
    }
    equationOfState
    {
        rho         1000; 
    }
    thermodynamics
    {
        Cp          4195;
        Hf          0;
    }
    transport
    {
        muw           1.0e-3;
        L             0.4;
        rhow          1000;
        zetaCoeff     460;
        alpha         2;
        beta          1;
    }
