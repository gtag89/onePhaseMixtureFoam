FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue Sw 0.01
);

expressions (
        Kabs 
     	{
		field Kabs;
		expression "0.0000001*rand(1)";
	}
);

