solvers
{
    "(Sw|divU)"
    {
        //solver           GAMG;
        //tolerance        1e-8;
        //relTol           0.01;
        //smoother         DICGaussSeidel;

        solver             PBiCGStab;
        preconditioner     DIC;
        tolerance          1.0e-8;
        relTol             0.01;
        minIter            1;

    }

    "(Sw|divU)Final"
    {
        $Sw;
        relTol             0.0;
        //minIter            10;

    }
    "(U|rho)"
    {        
        solver             PBiCGStab;
        preconditioner     DILU;
        tolerance          1.0e-8;
        relTol             0.01;
        //minIter            1;
        
        //solver          smoothSolver;
        //smoother        symGaussSeidel;
        //tolerance          1.0e-8;
        //relTol             0.01;
	//maxIter         10000;
    }
    "(U|rho)Final"
    {
        $U;
        relTol          0;
    }

}

