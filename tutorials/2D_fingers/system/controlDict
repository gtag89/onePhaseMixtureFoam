FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     overshootSatPimpleFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         150; //1400;//3000;

deltaT          1.0e-3;//1.0e-5;

writeControl    timeStep; //adjustableRunTime; 

writeInterval   1000; //10000;//1.0e+3; //200;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep   no;

maxCo            0.0001;
maxDeltaT        0.01;


