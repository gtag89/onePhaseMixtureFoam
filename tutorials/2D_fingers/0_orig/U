FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=64;scalar=64";
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    frontAndBack
    {
        type            symmetry;
    }
    rightAndLeft
    {
        type            empty;
    }
    top
    {

        //type                flowRateInletVelocity;
        //volumetricFlowRate  1.66791e-08; //m3/s
        //extrapolateProfile  yes;
        //value               uniform (0 0 0);
        type            fixedValue;
        value           uniform (0 0 -1.0e-3); 
    }
    bottom
    {
        //type            zeroGradient;
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }
}


